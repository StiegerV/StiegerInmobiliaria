@model StiegerModels.InquilinoModel
@{
    ViewBag.Title = "Detalle";
}

<h1>Editar Propietario</h1>

<form class="formulario-editar" action="@Url.Action("EditarInquilino", "Inquilino")" method="post">

    <input type="hidden" name="Id_inquilino"  asp-for="Id_inquilino" />

    <label for="Dni">Dni</label>
    <input type="text" id="Dni" asp-for="Dni" />

    <label for="Nombre">Nombre</label>
    <input type="text" id="Nombre" asp-for="Nombre" />

    <label for="Apellido">Apellido</label>
    <input type="text" id="Apellido" asp-for="Apellido" />

    <label for="Telefono">Telefono</label>
    <input type="text" id="Contacto" asp-for="Contacto" />

    <button type="submit">Guardar</button>
</form>